{
  "name": "nx-openapi-generator",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/nx-openapi-generator/bin",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/nx-openapi-generator",
        "main": "packages/nx-openapi-generator/bin/index.ts",
        "tsConfig": "packages/nx-openapi-generator/tsconfig.lib.json",
        "assets": ["packages/nx-openapi-generator/*.md"]
      }
    },
    "publish": {
      "command": "node tools/scripts/publish.mjs nx-openapi-generator {args.ver} {args.tag}",
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/nx-openapi-generator/**/*.ts",
          "packages/nx-openapi-generator/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/nx-openapi-generator/jest.config.ts"
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["nx-snakegio"]
}
